@model ICollection<WebApplication2.Models.CrsResult>

@{
    var studentNames = ViewBag.StudentNames as Dictionary<int, string>;
    var courseNames = ViewBag.CourseNames as Dictionary<int, string>;
}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Student Id</th>
        <th scope="col">Student Name</th>
        <th scope="col">Course Name</th>
        <th scope="col">Course Id</th>
        <th scope="col">Degree</th>
        <th scope="col" colspan="3"></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var i in Model)
    {
        <tr>
            <td>@i.StudentId</td>
            <td>@(studentNames?.GetValueOrDefault(i.StudentId ?? 0) ?? "Unknown")</td>
            <td>@(courseNames?.GetValueOrDefault(i.CourseId ?? 0) ?? "Unknown")</td>
            <td>@i.CourseId</td>
            <td>@i.Degree</td>
            <td><a asp-action="Edit" asp-controller="CrsResult" asp-route-id="@i.CrsResultId" class="btn btn-outline-info">Edit</a></td>
            <td><a asp-action="Details" asp-controller="CrsResult" asp-route-id="@i.CrsResultId" class="btn btn-outline-secondary">Details</a></td>
            <td><a asp-action="Delete" asp-controller="CrsResult" asp-route-id="@i.CrsResultId" class="btn btn-outline-danger">Delete</a></td>
        </tr>
    }
    </tbody>
</table>
<a asp-action="New" asp-controller="CrsResult" class="btn btn-primary">New</a>